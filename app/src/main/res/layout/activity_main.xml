<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:fab="http://schemas.android.com/apk/res-auto"

    tools:context=".Activitys.MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/fondoenfermeriaa">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="119dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="20dp"

                        android:layout_marginBottom="20dp"
                        android:background="@drawable/shape_background_button_idiomas">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/baloo"
                            android:padding="20dp"
                            android:text="@string/idioma"

                            android:textColor="@color/black"
                            android:textSize="20sp" />

                        <Button
                            android:id="@+id/language2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@android:color/transparent"
                            android:onClick="SeleccionarIdioma"
                            android:textColor="#FFFFFF"
                            android:visibility="gone"
                            tools:visibility="gone" />

                        <Button
                            android:id="@+id/language"
                            android:layout_width="wrap_content"

                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_marginBottom="-2dp"
                            android:background="#00000000"
                            android:fontFamily="@font/baloo"
                            android:onClick="SeleccionarIdioma"
                            android:padding="50dp"
                            android:text="@string/idioma"
                            android:textColor="@color/gnt_white"
                            android:textSize="20sp"
                            android:textStyle="normal|bold|italic"
                            tools:ignore="OnClick" />

                        <Button
                            android:id="@+id/button2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:onClick="lanzarcalculadora"
                            android:text="Calculadora"
                            android:visibility="gone"
                            tools:ignore="OnClick" />


                    </RelativeLayout>


                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/ic_drop" />

                    <Button
                        android:id="@+id/button3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:onClick="lanzarchat"
                        android:text="Button"
                        android:visibility="gone" />

                    <Button
                        android:id="@+id/button5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:onClick="lanzarcolor"
                        android:text="color"
                        android:visibility="gone"
                        tools:ignore="OnClick"
                        tools:visibility="gone" />


                </RelativeLayout>

                <Button
                    android:id="@+id/button4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="lanzarcie10"
                    android:text="Calculadora"
                    android:visibility="gone"
                    tools:ignore="OnClick" />


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginEnd="20dp"

                        android:layout_marginBottom="20dp"
                        android:background="@drawable/fon_moradito">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/baloo"
                            android:padding="20dp"
                            android:text="   NANDA 2021 - 2023"

                            android:textColor="@color/black"
                            android:textSize="20sp" />


                        <Button
                            android:id="@+id/nandatitulo"
                            android:layout_width="274dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="#00000000"
                            android:onClick="lanzarlistado"
                            android:padding="35dp"

                            android:text="diagnosticos de enfermeria
                            ESPAÑOL"
                            android:textColor="@color/black"
                            tools:ignore="OnClick" />

                    </RelativeLayout>


                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/libronanda" />


                </RelativeLayout>


                <com.google.android.gms.ads.AdView
                    android:id="@+id/adView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_gravity="center"

                    android:elevation="7dp"
                    android:visibility="gone"
                    ads:adSize="MEDIUM_RECTANGLE"
                    ads:adUnitId="@string/banner_ad_unit_id"></com.google.android.gms.ads.AdView>




                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="20dp"

                        android:layout_marginBottom="20dp"
                        android:background="@drawable/fon_verdesito">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/baloo"
                            android:padding="20dp"
                            android:text="        NOC 2020 - 2023"

                            android:textColor="@color/black"
                            android:textSize="20sp" />

                        <Button
                            android:id="@+id/nandatitulo2"
                            android:layout_width="274dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="#00000000"
                            android:onClick="lanzarlibro2"
                            android:padding="35dp"
                            android:text="diagnosticos de enfermeria
                            (6ª ed.) ESPAÑOL"
                            android:textColor="@color/black"
                            tools:ignore="OnClick" />

                    </RelativeLayout>


                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/noc2020" />


                </RelativeLayout>

                <com.nursing.nandapro.nicnoc.TemplateView
                    android:id="@+id/my_template"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="18dp"
                    android:visibility="gone"
                    app:gnt_template_type="@layout/gnt_medium_template_view"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/id_exit_title" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="20dp"

                        android:layout_marginBottom="20dp"
                        android:background="@drawable/fon_plomo">

                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/baloo"
                            android:padding="20dp"
                            android:text="        NIC 2020 - 2023"

                            android:textColor="@color/black"
                            android:textSize="20sp" />

                        <Button
                            android:id="@+id/nandatitulo3"
                            android:layout_width="274dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="#00000000"
                            android:onClick="lanzarlibro3"
                            android:padding="35dp"

                            android:text="diagnosticos de enfermeria
                            (7ª ed.) ESPAÑOL 100%"
                            android:textColor="@color/black"
                            tools:ignore="OnClick" />

                    </RelativeLayout>


                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/nic2020" />


                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="20dp"

                        android:layout_marginBottom="20dp"
                        android:background="@drawable/fon_celeste">

                        <TextView
                            android:id="@+id/nandatitulo6"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/baloo"
                            android:padding="20dp"
                            android:text="            Interrelaciones"

                            android:textColor="@color/black"
                            android:textSize="20sp" />

                        <Button
                            android:id="@+id/nandatitulo5"
                            android:layout_width="274dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="#00000000"
                            android:onClick="lanzarinterrelaciones"

                            android:padding="35dp"
                            android:text="nanda noc nic
                           100% 2022 - 2023"
                            android:textColor="@color/black"
                            tools:ignore="OnClick" />

                    </RelativeLayout>


                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/inte" />


                </RelativeLayout>





                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal" >

                            <ImageView
                                android:id="@+id/imageView3"
                                android:layout_width="90dp"
                                android:layout_height="90dp"
                                android:layout_gravity="center"
                                android:layout_marginStart="1dp"
                                android:layout_marginTop="1dp"
                                android:layout_marginEnd="10dp"
                                android:layout_marginBottom="20dp"

                                android:onClick="noads"
                                android:visibility="visible"
                                fab:srcCompat="@drawable/noads" />



                            <ImageView
                                android:id="@+id/imageView9"
                                android:layout_width="77dp"
                                android:layout_height="77dp"
                                android:layout_gravity="left"
                                android:layout_marginStart="1dp"
                                android:layout_marginTop="1dp"
                                android:layout_marginEnd="20dp"
                                android:layout_marginBottom="20dp"

                                android:onClick="lanzartutorial"

                                android:visibility="visible"
                                fab:srcCompat="@drawable/playvideo" />

                            <ImageView
                                android:id="@+id/imageView10"
                                android:layout_width="75dp"
                                android:layout_height="75dp"
                                android:layout_gravity="left"
                                android:layout_marginStart="1dp"
                                android:layout_marginTop="1dp"
                                android:layout_marginEnd="20dp"
                                android:layout_marginBottom="20dp"

                                android:onClick="lanzarcalculadoraweb"

                                android:visibility="visible"
                                fab:srcCompat="@drawable/calculadoraweb"
                                tools:ignore="OnClick" />

                            <ImageView
                                android:id="@+id/imageView2"
                                android:layout_width="90dp"
                                android:layout_height="90dp"
                                android:layout_gravity="left"
                                android:layout_marginStart="1dp"
                                android:layout_marginTop="15dp"
                                android:layout_marginEnd="10dp"
                                android:layout_marginBottom="1dp"

                                android:onClick="lanzarwalpapers"

                                fab:srcCompat="@drawable/fotos"
                                tools:ignore="OnClick" />

                            <ImageView
                                android:id="@+id/imageView"
                                android:layout_width="90dp"
                                android:layout_height="90dp"
                                android:layout_gravity="left"
                                android:layout_marginStart="1dp"
                                android:layout_marginTop="1dp"
                                android:layout_marginEnd="20dp"
                                android:layout_marginBottom="20dp"

                                android:onClick="rate"

                                android:visibility="visible"
                                fab:srcCompat="@drawable/ratestar" />

                        </LinearLayout>
                    </ScrollView>

                </LinearLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="20dp"

                        android:layout_marginBottom="20dp"
                        android:background="@drawable/fon_naranja2">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/baloo"
                            android:padding="20dp"
                            android:text="            CIE ! ICD - 10 - 11"

                            android:textColor="@color/black"
                            android:textSize="20sp" />

                        <Button
                            android:id="@+id/nandatitulo4"
                            android:layout_width="274dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:background="#00000000"
                            android:onClick="ic10"

                            android:padding="35dp"
                            android:text="Clasificación Internacional de Enfermedades
                            ESPAÑOL"
                            android:textColor="@color/black"
                            tools:ignore="OnClick" />

                    </RelativeLayout>


                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/icd" />


                </RelativeLayout>



            </LinearLayout>
        </ScrollView>


        <com.github.clans.fab.FloatingActionMenu
            android:id="@+id/social_floating_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="11dp"
            android:layout_marginRight="11dp"
            android:layout_marginBottom="11dp"
            android:visibility="visible"
            fab:menu_animationDelayPerItem="55"
            fab:menu_backgroundColor="@android:color/transparent"
            fab:menu_buttonSpacing="0dp"
            fab:menu_colorNormal="#da3c2f"
            fab:menu_colorPressed="#dc4b3f"
            fab:menu_colorRipple="#99d4d4d4"
            fab:menu_fab_label="follow me"
            fab:menu_fab_size="normal"
            fab:menu_icon="@drawable/fab_add"
            fab:menu_labels_colorNormal="#333"
            fab:menu_labels_colorPressed="#444"
            fab:menu_labels_colorRipple="#66efecec"
            fab:menu_labels_cornerRadius="3dp"
            fab:menu_labels_ellipsize="none"
            fab:menu_labels_hideAnimation="@anim/fab_slide_out_to_right"
            fab:menu_labels_margin="0dp"
            fab:menu_labels_maxLines="-1"
            fab:menu_labels_padding="8dp"
            fab:menu_labels_position="left"
            fab:menu_labels_showAnimation="@anim/fab_slide_in_from_right"
            fab:menu_labels_showShadow="true"
            fab:menu_labels_singleLine="false"
            fab:menu_labels_textColor="#f2f1f1"
            fab:menu_labels_textSize="15sp"
            fab:menu_openDirection="up"
            fab:menu_shadowColor="#66aff198"
            fab:menu_shadowRadius="4dp"
            fab:menu_shadowXOffset="1dp"
            fab:menu_shadowYOffset="4dp"
            fab:menu_showShadow="true">

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/share"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/sharecircle"
                app:fab_label=""
                fab:fab_size="mini" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/floating_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_home"
                app:fab_label=""
                fab:fab_size="mini" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/floating_rate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_rate"
                app:fab_label=""
                fab:fab_size="mini" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/floating_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/action_tw"
                app:fab_label=""
                fab:fab_size="mini" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/floating_face"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/action_fb"
                app:fab_label=""
                fab:fab_size="mini" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/floating_tiktok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/tiktok"
                app:fab_label=""
                fab:fab_size="mini" />

            <com.github.clans.fab.FloatingActionButton
                android:id="@+id/floating_inste"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/action_insta"
                app:fab_label="Andrespro100"
                fab:fab_size="mini" />
        </com.github.clans.fab.FloatingActionMenu>



    </RelativeLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navi"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/white"
        app:headerLayout="@layout/header"
        app:menu="@menu/drawemenu"
        tools:ignore="MissingConstraints">

    </com.google.android.material.navigation.NavigationView>



</androidx.drawerlayout.widget.DrawerLayout>